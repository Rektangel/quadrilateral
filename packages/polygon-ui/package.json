{
  "name": "@rektangel/polygon-ui",
  "version": "0.5.2",
  "description": "UI library for Rektangel",
  "author": "@ErikSamuelsson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Rektangel/quadrilateral.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "react-component"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit && eslint ./ --ext .ts,.tsx,.js",
    "prepare": "yarn run build",
